    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/pdfkit": "^0.14.0",
    "axios": "^1.10.0",

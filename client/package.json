{
  "name": "app-supel-client",
  "version": "1.0.0",
  "description": "Frontend do Sistema de Controle de Processos",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:ps": "node -e \"require('child_process').execSync('tsc', {stdio: 'inherit'}); require('child_process').execSync('vite build', {stdio: 'inherit'})\"",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "pwa:icons": "cd public && node generate-icons.cjs",
    "pwa:install-sharp": "npm install sharp",
    "pwa:setup": "npm run pwa:install-sharp && npm run pwa:icons",
    "pwa:check": "node build-pwa.js",
    "build:pwa": "npm run pwa:check && npm run build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.19",
    "@mui/x-data-grid": "^6.18.2",
    "@mui/x-date-pickers": "^6.18.2",
    "@tanstack/react-query": "^5.80.10",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "dayjs": "^1.11.10",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.5.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "recharts": "^3.0.2",
    "sharp": "^0.34.2",
    "xlsx": "^0.18.5",
    "yup": "^1.4.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.7"
  },
  "overrides": {
    "rollup": "4.43.0"
  }
}
